io </span><span id="w_3_p11">que </span><span id="w_4_p11">viva </span><span id="w_5_p11">con </span><span id="w_6_p11">su </span><span id="w_7_p11">abuela </span><span id="w_8_p11">que </span><span id="w_9_p11">se </span><span id="w_10_p11">hallaba </span><span id="w_11_p11">cautiva </span><span id="w_12_p11">de </span><span id="w_13_p11">una </span><span id="w_14_p11">mecedora </span><span id="w_15_p11">de </span><span id="w_16_p11">la </span><span id="w_17_p11">que </span><span id="w_18_p11">difcilmente </span><span id="w_19_p11">se </span><span id="w_20_p11">mova, </span><span id="w_21_p11">fue </span><span id="w_22_p11">el </span><span id="w_23_p11">tercer </span><span id="w_24_p11">nio </span><span id="w_25_p11">desaparecido. </span><span id="w_26_p11">No </span><span id="w_27_p11">asista </span><span id="w_28_p11">a </span><span id="w_29_p11">la </span><span id="w_30_p11">escuela </span><span id="w_31_p11">y </span><span id="w_32_p11">sola </span><span id="w_33_p11">vagar </span><span id="w_34_p11">por </span><span id="w_35_p11">la </span><span id="w_36_p11">ribera </span><span id="w_37_p11">o </span><span id="w_38_p11">por </span><span id="w_39_p11">el </span><span id="w_40_p11">bosque </span><span id="w_41_p11">cazando </span><span id="w_42_p11">ardillas </span><span id="w_43_p11">o </span><span id="w_44_p11">ranas </span><span id="w_45_p11">que </span><span id="w_46_p11">asaba </span><span id="w_47_p11">en </span><span id="w_48_p11">una </span><span id="w_49_p11">hoguera, </span><span id="w_50_p11">y </span><span id="w_51_p11">peleaba </span><span id="w_52_p11">con </span><span id="w_53_p11">otros </span><span id="w_54_p11">nios </span><span id="w_55_p11">o </span><span id="w_56_p11">les </span><span id="w_57_p11">robaba </span><span id="w_58_p11">discretamente </span><span id="w_59_p11">algn </span><span id="w_60_p11">juguete. </span><span id="w_61_p11">Nadie </span><span id="w_62_p11">lo </span><span id="w_63_p11">extra. </span><span id="w_64_p11">El </s