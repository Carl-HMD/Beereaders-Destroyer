ito </span><span id="w_264_p8">de </span><span id="w_265_p8">madera </span><span id="w_266_p8">tambin </span><span id="w_267_p8">apareci </span><span id="w_268_p8">bajo </span><span id="w_269_p8">la </span><span id="w_270_p8">cruz, </span><span id="w_271_p8">como </span><span id="w_272_p8">un </span><span id="w_273_p8">regalo </span><span id="w_274_p8">annimo.</span></p>  <p class="chapter_content"><span id="w_275_p8">Al </span><span id="w_276_p8">joven </span><span id="w_277_p8">sacerdote </span><span id="w_278_p8">que </span><span id="w_279_p8">llegaba </span><span id="w_280_p8">a </span><span id="w_281_p8">oficiar </span><span id="w_282_p8">la </span><span id="w_283_p8">misa </span><span id="w_284_p8">desde </span><span id="w_285_p8">la </span><span id="w_286_p8">ciudad, </span><span id="w_287_p8">cada </span><span id="w_288_p8">domingo </span><span id="w_289_p8">le </span><span id="w_290_p8">pareca </span><span id="w_291_p8">ms </span><span id="w_292_p8">evidente </span><span id="w_293_p8">que </span><span id="w_294_p8">la </span><span id="w_295_p8">anciana </span><span id="w_296_p8">haca </span><span id="w_297_p8">un </span><span id="w_298_p8">esfuerzo </span><span id="w_299_p8">mayor </span><span id="w_300_p8">que </span><span id="w_301_p8">el </span><span id="w_302_p8">domingo </span><span id="w_303_p8">anterior </span><span id="w_304_p8">para </span><span id="w_305_p8">llegar </span><span id="w_306_p8">al </span><span id="w_307_p8">atrio; </span><span id="w_308_p8">cada </span><span id="w_309_p8">vez </span><span id="w_310_p8">era </span><span id="w_311_p8">ms </span><span id="w_312_p8">notorio </span><span id="w_313_p8">cmo </span><span id="w_314_p8">arrastraba </span><span id="w_315_p8">su </span><span id="w_316_p8">pierna </span><span id="w_317_p8">derecha. </span><span id="w_318_p8">Sin </span><span id="w_319_p8">embargo, </span><span id="w_320_p8">se </span><span id="w_321_p8">mantena </span><span id="w_322_p8">firme </span><span id="w_323_p8">en </span