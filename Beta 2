aba </span><span id="w_47_p11">en </span><span id="w_48_p11">una </span><span id="w_49_p11">hoguera, </span><span id="w_50_p11">y </span><span id="w_51_p11">peleaba </span><span id="w_52_p11">con </span><span id="w_53_p11">otros </span><span id="w_54_p11">nios </span><span id="w_55_p11">o </span><span id="w_56_p11">les </span><span id="w_57_p11">robaba </span><span id="w_58_p11">discretamente </span><span id="w_59_p11">algn </span><span id="w_60_p11">juguete. </span><span id="w_61_p11">Nadie </span><span id="w_62_p11">lo </span><span id="w_63_p11">extra. </span><span id="w_64_p11">El </span><span id="w_65_p11">comisario </span><span id="w_66_p11">fingi </span><span id="w_67_p11">buscarlo, </span><span id="w_68_p11">pero </span><span id="w_69_p11">sin </span><span id="w_70_p11">nimo </span><span id="w_71_p11">y </span><span id="w_72_p11">sin </span><span id="w_73_p11">la </span><span id="w_74_p11">presin </span><span id="w_75_p11">que </span><span id="w_76_p11">le </span><span id="w_77_p11">exigieron </span><span id="w_78_p11">para </span><span id="w_79_p11">encontrar </span><span id="w_80_p11">a </span><span id="w_81_p11">los </span><span id="w_82_p11">otros </span><span id="w_83_p11">nios. </span><span id="w_84_p11">Sin </span><span id="w_85_p11">embargo, </span><span id="w_86_p11">la </span><span id="w_87_p11">nueva </span><span id="w_88_p11">desaparicin </span><span id="w_89_p11">recrudeci </span><span id="w_90_p11">el </span><span id="w_91_p11">miedo. </span><span id="w_92_p11">No </span><span id="w_93_p11">dejaban </span><span id="w_94_p11">que </span><span id="w_95_p11">la </span><span id="w_96_p11">noche </span><span id="w_97_p11">alcanzara </span><span id="w_98_p11">a </span><span id="w_99_p11">caer </span><span id="w_100_p11">cuando </span><span id="w_101_p11">las </span><span id="w_102_p11">madres </span><span id="w_103_p11">ya </span><span id="w_104_p11">tenan </span><span id="w_105_p11">encerrados </span><span id="w_106_p11">a </span><span id="w_107_p11"