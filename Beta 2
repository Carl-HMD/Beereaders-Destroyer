a </span><span id="w_62_p7">echarle </span><span id="w_63_p7">un </span><span id="w_64_p7">vistazo. </span><span id="w_65_p7">Un </span><span id="w_66_p7">quinqu </span><span id="w_67_p7">encendido </span><span id="w_68_p7">alumbraba </span><span id="w_69_p7">el </span><span id="w_70_p7">lugar </span><span id="w_71_p7">que </span><span id="w_72_p7">se </span><span id="w_73_p7">hallaba </span><span id="w_74_p7">ordenado: </span><span id="w_75_p7">la </span><span id="w_76_p7">cama, </span><span id="w_77_p7">el </span><span id="w_78_p7">bur, </span><span id="w_79_p7">el </span><span id="w_80_p7">ropero, </span><span id="w_81_p7">un </span><span id="w_82_p7">bal, </span><span id="w_83_p7">un </span><span id="w_84_p7">gran </span><span id="w_85_p7">reloj </span><span id="w_86_p7">de </span><span id="w_87_p7">pared, </span><span id="w_88_p7">sandalias, </span><span id="w_89_p7">cobertores, </span><span id="w_90_p7">las </span><span id="w_91_p7">almohadas. </span><span id="w_92_p7">Otro </span><span id="w_93_p7">hombre </span><span id="w_94_p7">subi </span><span id="w_95_p7">por </span><span id="w_96_p7">la </span><span id="w_97_p7">escalera </span><span id="w_98_p7">a </span><span id="w_99_p7">la </span><span id="w_100_p7">habitacin </span><span id="w_101_p7">de </span><span id="w_102_p7">arriba </span><span id="w_103_p7">y </span><span id="w_104_p7">empuj </span><span id="w_105_p7">suavemente </span><span id="w_106_p7">la </span><span id="w_107_p7">puerta. </span><span id="w_108_p7">Solamente </span><span id="w_109_p7">encontr </span><span id="w_110_p7">un </span><span id="w_111_p7">cuarto </span><span id="w_112_p7">vaco </span><span id="w_113_p7">sin </span><span id="w_114_p7">ventanas. </span><span id="w_115_p7">Baj </span><span id="w_116_p7">y </span><span id="w_117_p7">le </span><span id="w_118_p7">hizo </span><span id="w_119_p7">una </span><span id="w_120_p7">sea </span><span id="w_121_p7">al </span><span id="w_122_p7">padre </span><span id="w_123_p7">de </span><spa